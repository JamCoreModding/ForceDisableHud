{
  "schemaVersion": 1,
  "id": "force-disable-hud",
  "version": "${version}",
  "name": "Force Disable HUD",
  "description": "Allows servers to forcibly disable a clients HUD.",
  "authors": [
    "Jamalam"
  ],
  "contact": {
    "sources": "https://github.com/JamCoreModding/ForceDisableHud",
    "issues": "https://github.com/JamCoreModding/ForceDisableHud/issues"
  },
  "license": "MIT",
  "icon": "assets/force-disable-hud/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.jamalam360.force.disable.hud.ForceDisableHudInit"
    ],
    "client": [
      "io.github.jamalam360.force.disable.hud.ForceDisableHudClient"
    ]
  },
  "mixins": [
    "force-disable-hud.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.12.5",
    "fabric": "*"
  },
  "custom": {
    "notify_gradle_properties_url": "https://raw.githubusercontent.com/JamCoreModding/ForceDisableHud/main/gradle.properties",
    "notify_gradle_properties_key": "mod_version"
  }
}
